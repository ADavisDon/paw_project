    <table>
      <%= form_with(model: @event) do |f| %>
        <tr>
          <th><%= f.label :event_code %></th>
          <td><%= f.text_field :event_code, required: true %></td>
        </tr>
        <tr>
          <th><%= f.label :event_year %></th>
          <td><%= f.number_field :event_year, required: true %></td>
        </tr>
        <tr>
          <th><%= f.label :event_location %></th>
          <td><%= f.text_field :event_location %></td>
        </tr>
        <tr>
          <th><%= f.label "Currently set" %></th>
          <td>Set this event? <%= f.check_box :set%></td>
        </tr>
        <tr><td colspan="3"><%= f.submit %></td></tr>
      <% end %>
    </table>